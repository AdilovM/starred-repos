module.exports = "<div class=\"inner-popover\">\n    <div class=\"arrow\"></div>\n    <div class=\"content clear\">\n        <div class=\"left\">\n            <img class=\"user-img-20 author-image\" src=\"{{author.image}}\" />\n        </div>\n        <div class=\"note-content left\">\n            <div class=\"author-name\"><strong>{{author.fullName}}</strong></div>\n            <div class=\"note-static\">\n                <div class=\"note-body\">\n                    {{{formattedBody}}}\n                    {{#truncated}}\n                        <a href=\"javascript:;\" class=\"more-link\">(more)</a>\n                    {{/truncated}}\n                </div>\n                <div class=\"right footer note-stats\">\n                    <span class=\"ss-icon-heart note-stat note-stat-likes{{#userVote}}{{#value}} liked{{/value}}{{/userVote}}\"{{^numLikes}} style=\"display: none;\"{{/numLikes}}>{{numLikes}}</span>\n                </div>\n                <div class=\"footer\">\n                    {{^isGuest}}\n                        <a href=\"javascript:;\" class=\"note-like alternate\">Like</a>\n                    {{/isGuest}}\n                    {{#canModify}}\n                        <span class=\"middot\">&middot;</span>\n                        <a href=\"javascript:;\" class=\"edit-button alternate\">Edit</a>\n                        <span class=\"middot\">&middot;</span>\n                        <a href=\"javascript:;\" class=\"delete-button alternate\">Delete</a>\n                    {{/canModify}}\n                    {{#canMakePrivate}}\n                        <span class=\"middot\">&middot;</span>\n                        <a href=\"javascript:;\" class=\"make-private alternate\">Make Private</a>\n                    {{/canMakePrivate}}\n                </div>\n                {{^privacy_status}}\n                    <div class=\"privacy-status ss-icon-lock\">Only Me</div>\n                {{/privacy_status}}\n            </div>\n            {{#canModify}}\n                <div class=\"note-edit-form-wrapper\"></div>\n            {{/canModify}}\n\n        </div>\n    </div>\n</div>\n"